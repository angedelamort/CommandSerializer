<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <Version>0.1.1</Version>
    <Authors>Sauleil Menard-Lamarre</Authors>
    <Company>none</Company>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageProjectUrl>https://github.com/angedelamort/CommandSerializer</PackageProjectUrl>
    <RepositoryUrl>https://github.com/angedelamort/CommandSerializer</RepositoryUrl>
    <PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
    <PackageTags>netcore c# arguments command line</PackageTags>
    <RepositoryType>git</RepositoryType>
    <Description>C# command-line argument parsing deserialization from attributes.</Description>
    <Copyright>Copyright 2020 sauleil</Copyright>
    <NeutralLanguage>en</NeutralLanguage>
  </PropertyGroup>

  <ItemGroup>
    <None Include="LICENSE.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
